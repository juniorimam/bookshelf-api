{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-else-return" : "off",
        "no-unneeded-ternary" : "off",
        "linebreak-style": ["error", "windows"],
        "import/extensions": ["error", "always", {"ignorePackages": true} ]
    }
}